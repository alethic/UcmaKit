<?xml version="1.0" encoding="utf-8" ?>

<configuration>

    <configSections>
        <section
            name="UcmaKit.Rtc"
            type="UcmaKit.Rtc.RtcConfigurationSection, UcmaKit.Rtc" />
    </configSections>

    <UcmaKit.Rtc>
        <applications>
            <add
                applicationId="urn:application:isi-acd"
                userAgent="ISI Automated Call Distribution"
                type="UcmaKit.Rtc.Acd.AcdApplication, UcmaKit.Rtc.Acd" />
        </applications>
    </UcmaKit.Rtc>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5" />
    </startup>

</configuration>