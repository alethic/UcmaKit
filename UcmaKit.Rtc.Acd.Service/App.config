<?xml version="1.0" encoding="utf-8" ?>

<configuration>

    <configSections>
        <section
            name="isi.rtc"
            type="ISI.Rtc.RtcConfigurationSection, ISI.Rtc" />
    </configSections>

    <isi.rtc>
        <applications>
            <add
                applicationId="urn:application:isi-acd"
                userAgent="ISI Automated Call Distribution"
                type="ISI.Rtc.Acd.AcdApplication, ISI.Rtc.Acd" />
        </applications>
    </isi.rtc>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5" />
    </startup>

</configuration>